{"components/products/card-qty":"{{#if theme_settings.show_product_quantity_box}}\r\n    <div class=\"form-field form-field--increments\">\r\n        <label class=\"form-label form-label--alternate is-srOnly\" for=\"qty_{{id}}_{{incrementVar 'qtyBoxUID'}}\">{{lang 'products.quantity'}}</label>\r\n        <div class=\"form-increment\" data-card-quantity-change>\r\n            <button class=\"button button--icon\" data-action=\"dec\">\r\n                <span class=\"is-srOnly\">{{lang 'products.quantity_decrease'}}</span>\r\n                <i class=\"icon\" aria-hidden=\"true\">\r\n                    <svg>\r\n                        <use xlink:href=\"#icon-minus\"/>\r\n                    </svg>\r\n                </i>\r\n            </button>\r\n            <input class=\"form-input form-input--incrementTotal\"\r\n                id=\"qty_{{id}}_{{getVar 'qtyBoxUID'}}\"\r\n                name=\"qty_{{id}}\"\r\n                type=\"tel\"\r\n                value=\"{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}\"\r\n                data-quantity-min=\"{{min_purchase_quantity}}\"\r\n                data-quantity-max=\"{{max_purchase_quantity}}\"\r\n                min=\"1\"\r\n                pattern=\"[0-9]*\"\r\n                aria-live=\"polite\">\r\n            <button class=\"button button--icon\" data-action=\"inc\">\r\n                <span class=\"is-srOnly\">{{lang 'products.quantity_increase'}}</span>\r\n                <i class=\"icon\" aria-hidden=\"true\">\r\n                    <svg>\r\n                        <use xlink:href=\"#icon-add\"/>\r\n                    </svg>\r\n                </i>\r\n            </button>\r\n        </div>\r\n    </div>\r\n{{/if}}\r\n"}
